{
  "name": "trinh-ui",
  "version": "0.1.2",
  "description": "A React component library focused on minimal aesthetics, vivid Framer Motion animations, and strict accessibility",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib && npm run build:types",
    "build:lib": "BUILD_MODE=library vite build",
    "build:types": "tsc --project tsconfig.build.json",
    "build:docs": "vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "framer-motion",
    "animation",
    "accessible",
    "typescript",
    "tailwind",
    "theme"
  ],
  "author": "Khoa Ma",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/khoama/trinh-ui.git"
  },
  "bugs": {
    "url": "https://github.com/khoama/trinh-ui/issues"
  },
  "homepage": "https://trinh-ui.khoa.ma",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.555.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
